{
  "expo": {
    "name": "Campos Solo",
    "slug": "Compostagem",
    "version": "1.0.1",
    "scheme": "pucci",
    "orientation": "portrait",
    "icon": "./assets/campos_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/campos_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.campossolo.puccicompostagem",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/campos_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.campossolo.puccicompostagem"
    },
    "web": {
      "favicon": "./assets/campos_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2c689a26-f5e9-40b3-8966-77ef3e275365"
      }
    }
  }
}
